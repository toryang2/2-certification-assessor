	<detail>
		<band height="440" splitType="Stretch">
			<element kind="textField" uuid="9e453b17-a0a1-41a4-aa00-2bc257c14c72" stretchType="NoStretch" x="0" y="71" width="443" height="229" markup="styled" fontName="Bookman Old Style" fontSize="12.0" linkTarget="Self" hTextAlign="Justified">
				<paragraph firstLineIndent="20"/>
				<expression><![CDATA["THIS IS TO CERTIFY that " + 
"<style isBold=\"true\">" + 
(assessor.component.report.StatusUtil.getParentStatus($F{marital_status}).isEmpty() 
    ? "" 
    : assessor.component.report.StatusUtil.getParentStatus($F{marital_status}) + " "
) + " " +  $F{parent_display} + "</style> " + 
" of Barangay " + "<style isBold=\"true\">" + $F{barangay} + "</style> " +
" this municipality has no declared real property ownership such as building, land, and machinery as far as our record is concerned.\n\n" +

"THIS CERTIFICATION is being done to facilitate their request for financial/medical assistance of " + 
(
  // Condition 1: Married + spouse = blank
  ($F{parent_sex_if_single} != null && $F{parent_sex_if_single}.equals("Married") &&
   $F{relationship} != null && $F{relationship}.equalsIgnoreCase("spouse"))
    ? ""
    // Condition 2: Male/Female + self = blank
    : (
      ($F{parent_sex_if_single} != null &&
       ($F{parent_sex_if_single}.equalsIgnoreCase("Male") || $F{parent_sex_if_single}.equalsIgnoreCase("Female")) &&
       $F{relationship} != null && $F{relationship}.equalsIgnoreCase("self"))
        ? ""
    // Condition 3: Live-in + husband/wife or self = blank
    	: (
      	  ($F{parent_sex_if_single} != null &&
      		 $F{parent_sex_if_single}.equalsIgnoreCase("Live-in") &&
      		 $F{relationship} != null && 
      		 ($F{relationship}.equalsIgnoreCase("husband") || $F{relationship}.equalsIgnoreCase("wife") || 
      		 	$F{relationship}.equalsIgnoreCase("self")
      		 ))
      		  ? ""
        // Default case
       		 : (
          		  (assessor.component.report.GrammarHelper.getPossessive($F{parent_sex_if_single}).isEmpty()
             		 ? ""
           	   		 : assessor.component.report.GrammarHelper.getPossessive($F{parent_sex_if_single}) + " "
            		) +
           		 (
              		($F{relationship} == null || $F{relationship}.isEmpty())
                	? ""
                	: $F{relationship} + " "
                	)
            	 )
          )
    )
) + 
"  <style isBold=\"true\">" + $F{patient} + " (Patient)</style> at " + $F{hospital} + " â€“ " + assessor.component.report.TextNormalizer.toProperCase($F{hospital_address}) + ".\n\n" +

"Done and Given this " + 
	( ($F{certification_date} != null) ? 
  		assessor.component.report.DateUtil.formatCertificationDate($F{certification_date}) : 
 	   "[DATE MISSING]"
	) +
" at the Office of the Municipal Assessor, Poblacion, Kitaotao, Bukidnon."
]]></expression>
				<property name="net.sf.jasperreports.awt.justify.last.line" value="false"/>
				<property name="com.jaspersoft.studio.unit.firstLineIndent" value="px"/>
			</element>
			<element kind="staticText" uuid="24cccd8c-34bd-4632-8b8f-36b325f835f2" x="0" y="40" width="443" height="15" fontName="Bookman Old Style" fontSize="12.0">
				<text><![CDATA[TO WHOM IT MAY CONCERN:]]></text>
			</element>
			<element kind="textField" uuid="4fc81109-490d-4764-9b03-9a3d5028d34d" x="200" y="325" width="243" height="15" markup="styled" fontName="Bookman Old Style" fontSize="12.0" linkType="None" linkTarget="Self" bold="true" hTextAlign="Center">
				<expression><![CDATA[($F{signatory} == null ? "" : $F{signatory}.replaceFirst("(,.*)", "<style isBold=\"false\"><font size='10'>$1</font></style>"))]]></expression>
			</element>
