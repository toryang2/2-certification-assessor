	<query language="sql"><![CDATA[SELECT id,
	`marital_status`,
	`parent_guardian`,
	`parent_guardian_2`,
    /* New computed column */
    CASE 
        WHEN `parent_guardian_2` IS NOT NULL AND TRIM(`parent_guardian_2`) <> ''
        THEN CONCAT(COALESCE(`parent_guardian`, ''), ' AND ', `parent_guardian_2`)
        ELSE COALESCE(`parent_guardian`, '')
    END AS `parent_display`,
	`parent_sex_if_single`,
	`barangay`,
	`patient`,
	`hospital`,
	`relationship`,
	`hospital_address`,
	`certification_date`,
	`certification_time`,
