	<parameter name="SelectedIDs" class="java.lang.List"/>
	<query language="sql"><![CDATA[SELECT id,
	`objid`,
	`marital_status`,
	`requestor`,
	`owner`,
	`spouse`,
	/* New computed column*/
	CASE
		WHEN `spouse` IS NOT NULL AND TRIM(`spouse`) <> ''
		THEN CONCAT(COALESCE(`owner`, ''), ' AND ', `spouse`)
		ELSE COALESCE(`owner`, '')
	END AS `full_spouse`,
	`purpose`,
	`requested_date`,
	`requested_time`,
	`amount_paid`,
	`receipt_no`,
	`receipt_date_issued`,
	`place_issued`,
	`type`,
	`user_initials`,
	`signatory`
FROM certificationdb.reports_total_landholding
WHERE $X{IN, id, SelectedIDs}]]></query>
